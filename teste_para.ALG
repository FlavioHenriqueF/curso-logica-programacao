Algoritmo "teste_para"

Var

   i, n,soma,n1 : inteiro

Inicio

      escreva("Quantos numeros serao digitados? ")
      leia(n)
      
      soma <- 0
      
      para i de 1 ate n faca

           escreva("Digite um numero: ")
           leia(n1)
           soma <- soma + n1
           
      fimpara
      
      escreval
      escreval("SOMA = ",soma)

      
      

Fimalgoritmo